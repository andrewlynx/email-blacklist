{
    "name": "andrewlynx/email-blacklist",
    "description": "Component for Laravel 5.7 that manages blacklisted emails",
    "type": "library",
    "require": {
        "laravel/framework": "^5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Melnik",
            "email": "andrew.melnik.f@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Andrewlynx\\EmailBlacklist\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Andrewlynx\\EmailBlacklist\\Providers\\EmailBlacklistServiceProvider"
            ],
            "aliases": {
                "EmailBlacklist": "Andrewlynx\\EmailBlacklist\\EmailBlacklist"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Andrewlynx\\EmailBlacklist\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    }
}
